.pageIndex {
  overflow: hidden;
}
.pageIndex .bar {
  height: 25px;
  width: 100%;
  background-color: #F47119;
}
.pageIndex .header {
  height: 90rpx;
  width: 1000rpx;
  background-color: #F47119;
}
.pageIndex .header .contentMain {
  margin: 0 30rpx;
}
.pageIndex .header .contentMain .status_bar {
  color: #FFFFFF;
  font-weight: 900;
  font-size: 30rpx;
}
.pageIndex .header .contentMain .status_bar > text {
  display: inline-block;
  width: 750rpx;
  height: 40rpx;
  margin-top: 20rpx;
}
.pageIndex .header .contentMain .status_bar > text .iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
  margin-right: 10rpx;
}
.pageIndex .header .contentMain .status_bar > text .sub_bar {
  margin-top: 20rpx;
  margin-left: 15rpx;
  font-size: 20rpx;
  line-height: 40rpx;
  vertical-align: -5rpx;
}

